{
  "name": "agent-browser-protocol",
  "version": "0.1.2",
  "description": "Agent Browser Protocol - Deterministic AI agent browser control at the engine level",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agent-browser-protocol": "./dist/bin/abp.js",
    "abp-debug": "./dist/bin/debug.js"
  },
  "files": [
    "dist/",
    ".claude-plugin/",
    ".mcp.json",
    "skills/",
    "src/debug-ui.html"
  ],
  "scripts": {
    "build": "tsup",
    "generate-skill": "node scripts/generate-skill.mjs",
    "prebuild": "node scripts/generate-skill.mjs",
    "postinstall": "node dist/install.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "browser",
    "automation",
    "ai",
    "agent",
    "chromium",
    "cdp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/theredsix/agent-browser-protocol.git"
  },
  "author": "Han Wang <han.wang.2718@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "sharp": "^0.34.5"
  }
}
